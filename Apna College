#include<iostream>
using namespace std;

int main(){
    cout<<"*****\n";
    cout<<"****\n";
    cout<<"***\n";
    cout<<"**\n";
    cout<<"*\n";
	int a = 5;
	int b= 10;
	
	cout<<"a = "<< a <<endl;
	cout<<"b = "<< b <<endl;
    return 0;
}


#include<iostream>
using namespace std;

int main(){
	int a;
	int b;
	//input a
	cout<<"enter a : ";
	cin>>a;
	
	//input b
	cout<<"enter b : ";
	cin>>b;
	
	//calculate sum
	int sum = a + b;
	cout<<"sum = "<<sum<<endl;
	
	int prod = a * b;
	cout<<"prod = "<<prod<<endl;
	
	return 0;
}


#include<iostream>
using namespace std;

int main(){
	int eng;
	int sci;
	int math;
	
	cout<<"enter english marks : ";
	cin>>eng;
	cout<<"enter sci marks : ";
	cin>>sci;
	cout<<"enter math marks : ";
	cin>>math;
	
	int avg = (eng + sci + math) / 3;
	cout<<"avg marks = "<<avg<<endl;

	return 0;
}


